error);\n      });\n    }\n\n  },\n\n  // beforeCreate(){\n  //   this.getProducts();\n  // },\n  mounted() {\n    this.getProducts();\n    this.exampleItems = this.items;\n    Swiper.use(Autoplay);\n    const swiper = new Swiper(\".swiper\", {\n      loop: true,\n      direction: \"horizontal\",\n      slidesPerView: 1,\n      modules: [Autoplay],\n      speed: 700,\n      autoplay: {\n        delay: 3000,\n        disableOnInteraction: false,\n        pauseOnMouseEnter: true,\n        el: \".swiper-slide\"\n      },\n      on: {\n        init() {\n          this.el.addEventListener(\"mouseenter\", () => {\n            this.autoplay.stop();\n          });\n          this.el.addEventListener(\"mouseleave\", () => {\n            this.autoplay.start();\n          });\n        }\n\n      }\n    });\n    swiper.autoplay.start();\n  }\n\n};","map":{"version":3,"mappings":"AA6KA,SAASA,MAAT,EAAiBC,QAAjB,QAAiC,QAAjC;AACA,OAAOC,WAAP,MAAwB,8BAAxB;AACA,OAAOC,UAAP,MAAuB,6BAAvB;AACA,eAAe;AACbC,OAAK,EAAE,CACL,aADK,EAEL,OAFK,EAGL,WAHK,EAIL,eAJK,EAKL,mBALK,EAML,YANK,EAOL,QAPK,EAQL,aARK,EASL,gBATK,EAUL,cAVK,CADM;AAabC,YAAU,EAAE;AACVH,eADU;AAEVF,UAFU;AAGVG;AAHU,GAbC;;AAkBbG,MAAI,GAAG;AACL,WAAO;AACLC,kBAAY,EAAE,EADT;AAELC,iBAAW,EAAE,EAFR;AAGLC,oBAAc,EAAE,EAHX;AAILC,kBAAY,EAAE;AAJT,KAAP;AAMD,GAzBY;;AA0BbC,SAAO,EAAE;AACP;AACA;AACA;AACA;AACAC,iBAAa,GAAG;AACd;AACA,UAAIC,YAAW,GAAIC,SAAS,CAAC,CAAD,CAA5B;;AACA,UAAIC,KAAK,CAAC,KAAKC,QAAN,CAAL,IAAwB,KAAKA,QAAL,GAAgB,CAA5C,EAA+C;AAC7C,aAAKA,QAAL,GAAgB,CAAhB;AACF;;AAEA,YAAMC,QAAO,GAAI;AACfC,aAAK,EAAE,KAAKV,WAAL,CAAiBK,YAAjB,CADQ;AAEfG,gBAAQ,EAAE,KAAKA;AAFA,OAAjB;AAIA,WAAKG,MAAL,CAAYC,MAAZ,CAAmB,WAAnB,EAAgCH,QAAhC;AACA,WAAKI,MAAL,CAAYC,OAAZ,CAAqB,GAAE,KAAKd,WAAL,CAAiBK,YAAjB,EAA+BU,IAAI,gBAA1D;AACD,KAlBM;;AAmBPC,qBAAiB,GAAG;AAClB;AACA,UAAIX,YAAW,GAAIC,SAAS,CAAC,CAAD,CAA5B;;AACA,UAAIC,KAAK,CAAC,KAAKC,QAAN,CAAL,IAAwB,KAAKA,QAAL,GAAgB,CAA5C,EAA+C;AAC7C,aAAKA,QAAL,GAAgB,CAAhB;AACF;;AAEA,YAAMS,YAAW,GAAI;AACnBP,aAAK,EAAE,KAAKV,WAAL,CAAiBK,YAAjB,CADY;AA