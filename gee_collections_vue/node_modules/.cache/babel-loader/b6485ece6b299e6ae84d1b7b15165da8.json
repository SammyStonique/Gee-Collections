{"ast":null,"code":"export default {\n  props: ['getUserDetails', 'userDetails'],\n\n  data() {\n    return {\n      first_name: '',\n      last_name: '',\n      // email: null,\n      // phone_number: null,\n      birthdate: '',\n      address: '',\n      county: '',\n      city: '',\n      gender: '',\n      errors: []\n    };\n  },\n\n  methods: {\n    postProfileData() {\n      this.errors = [];\n\n      if (this.first_name === '' && this.last_name === '' && this.birthdate === '' && this.gender === '' && this.city === '' && this.county === '' && this.address === '') {\n        this.errors.push('Please fill in the details!');\n      } else {\n        if (this.first_name === '') {\n          this.errors.push('First Name is missing');\n        }\n\n        if (this.last_name === '') {\n          this.errors.push('Last Name is missing');\n        } // if(this.phone_number ===''){\n        //     this.errors.push('Phone Number is missing')\n        // }\n\n\n        if (this.birthdate === '') {\n          this.errors.push('Date of birth missing');\n        }\n\n        if (this.gender === '') {\n          this.errors.push('Please select your gender');\n        }\n\n        if (this.city === '') {\n          this.errors.push('Please select your gender');\n        }\n\n        if (this.county === '') {\n          this.errors.push('Please select your gender');\n        }\n\n        if (this.address === '') {\n          this.errors.push('Address details missing');\n        }\n      }\n\n      if (!this.errors.length) {\n        let formData = {\n          first_name: this.first_name,\n          last_name: this.last_name,\n          birthdate: this.birthdate,\n          address: this.address,\n          county: this.county,\n          city: this.city,\n          gender: this.gender\n        }; // let formData = new FormData()\n        // formData.append('first_name',this.first_name);\n        // formData.append('last_name',this.last_name);\n        // formData.append('birthdate',this.birthdate);\n        // formData.append('gender',this.gender);\n        // formData.append('city',this.city);\n        // formData.append('county',this.county);\n        // formData.append('address',this.address);\n\n        this.axios.put(`/api/v1/user-profile/?${this.userDetails.email}`, formData).then(response => {\n          console.log(response.data);\n          this.$toast.success('Profile Succesfully Updated', {\n            duration: 5000,\n            dismissible: true\n          });\n        }).catch(error => {\n          console.log(error);\n        });\n      }\n    }\n\n  },\n\n  mounted() {\n    this.getUserDetails();\n    this.postProfileData();\n  }\n\n};","map":{"version":3,"mappings":"AA6LA,eAAe;AACXA,OAAK,EAAC,CAAC,gBAAD,EAAkB,aAAlB,CADK;;AAEXC,MAAI,GAAE;AACF,WAAM;AACFC,gBAAU,EAAC,EADT;AAEFC,eAAS,EAAE,EAFT;AAGF;AACA;AACAC,eAAS,EAAE,EALT;AAMFC,aAAO,EAAE,EANP;AAOFC,YAAM,EAAE,EAPN;AAQFC,UAAI,EAAE,EARJ;AASFC,YAAM,EAAE,EATN;AAUFC,YAAM,EAAC;AAVL,KAAN;AAYH,GAfU;;AAgBXC,SAAO,EAAC;AACJC,mBAAe,GAAE;AACb,WAAKF,MAAL,GAAc,EAAd;;AACA,UAAG,KAAKP,UAAL,KAAmB,EAAnB,IAAuB,KAAKC,SAAL,KAAkB,EAAzC,IAA6C,KAAKC,SAAL,KAAkB,EAA/D,IAAmE,KAAKI,MAAL,KAAe,EAAlF,IAAsF,KAAKD,IAAL,KAAa,EAAnG,IAAuG,KAAKD,MAAL,KAAe,EAAtH,IAA0H,KAAKD,OAAL,KAAgB,EAA7I,EAAgJ;AAC5I,aAAKI,MAAL,CAAYG,IAAZ,CAAiB,6BAAjB;AACH,OAFD,MAEK;AACD,YAAG,KAAKV,UAAL,KAAmB,EAAtB,EAAyB;AACrB,eAAKO,MAAL,CAAYG,IAAZ,CAAiB,uBAAjB;AACJ;;AACA,YAAG,KAAKT,SAAL,KAAkB,EAArB,EAAwB;AACpB,eAAKM,MAAL,CAAYG,IAAZ,CAAiB,sBAAjB;AACJ,SANC,CAOD;AACA;AACA;;;AACA,YAAG,KAAKR,SAAL,KAAkB,EAArB,EAAwB;AACpB,eAAKK,MAAL,CAAYG,IAAZ,CAAiB,uBAAjB;AACJ;;AACA,YAAG,KAAKJ,MAAL,KAAe,EAAlB,EAAqB;AACjB,eAAKC,MAAL,CAAYG,IAAZ,CAAiB,2BAAjB;AACJ;;AACA,YAAG,KAAKL,IAAL,KAAa,EAAhB,EAAmB;AACf,eAAKE,MAAL,CAAYG,IAAZ,CAAiB,2BAAjB;AACJ;;AACA,YAAG,KAAKN,MAAL,KAAe,EAAlB,EAAqB;AACjB,eAAKG,MAAL,CAAYG,IAAZ,CAAiB,2BAAjB;AACJ;;AACA,YAAG,KAAKP,OAAL,KAAgB,EAAnB,EAAsB;AAClB,eAAKI,MAAL,CAAYG,IAAZ,CAAiB,yBAAjB;AACJ;AACJ;;AACA,UAAG,CAAC,KAAKH,MAAL,CAAYI,MAAhB,EAAuB;AAC