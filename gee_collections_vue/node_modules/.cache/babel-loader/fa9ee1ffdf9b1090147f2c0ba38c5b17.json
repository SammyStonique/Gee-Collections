{"ast":null,"code":"import paginate from 'jw-paginate';\nconst defaultLabels = {\n  first: 'First',\n  last: 'Last',\n  previous: 'Previous',\n  next: 'Next'\n};\nconst defaultStyles = {\n  ul: {\n    margin: 0,\n    padding: 0,\n    display: 'inline-block'\n  },\n  li: {\n    listStyle: 'none',\n    display: 'inline',\n    textAlign: 'center'\n  },\n  a: {\n    cursor: 'pointer',\n    padding: '6px 12px',\n    display: 'block',\n    float: 'left'\n  }\n};\nexport default {\n  props: {\n    items: {\n      type: Array,\n      required: true\n    },\n    initialPage: {\n      type: Number,\n      default: 1\n    },\n    pageSize: {\n      type: Number,\n      default: 10\n    },\n    maxPages: {\n      type: Number,\n      default: 10\n    },\n    labels: {\n      type: Object,\n      default: () => defaultLabels\n    },\n    styles: {\n      type: Object\n    },\n    disableDefaultStyles: {\n      type: Boolean,\n      default: false\n    }\n  },\n\n  data() {\n    return {\n      pager: {},\n      ulStyles: {},\n      liStyles: {},\n      aStyles: {}\n    };\n  },\n\n  created() {\n    // if (!this.$listeners.changePage) {\n    //     throw 'Missing required event listener: \"changePage\"';\n    // }\n    // set default styles unless disabled\n    if (!this.disableDefaultStyles) {\n      this.ulStyles = defaultStyles.ul;\n      this.liStyles = defaultStyles.li;\n      this.aStyles = defaultStyles.a;\n    } // merge custom styles with default styles\n\n\n    if (this.styles) {\n      this.ulStyles = { ...this.ulStyles,\n        ...this.styles.ul\n      };\n      this.liStyles = { ...this.liStyles,\n        ...this.styles.li\n      };\n      this.aStyles = { ...this.aStyles,\n        ...this.styles.a\n      };\n    } // set to initial page\n\n\n    this.setPage(this.initialPage);\n  },\n\n  methods: {\n    setPage(page) {\n      const {\n        items,\n        pageSize,\n        maxPages\n      } = this; // get new pager object for specified page\n\n      const pager = paginate(items.length, page, pageSize, maxPages); // get new page of items from items array\n\n      const pageOfItems = items.slice(pager.startIndex, pager.endIndex + 1); // update pager\n\n      this.pager = pager; // emit change page event to parent component\n\n      this.$emit('changePage', pageOfItems);\n    }\n\n  },\n  watch: {\n    items() {\n      this.setPage(this.initialPage);\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAqBI,OAAOA,QAAP,MAAqB,aAArB;AACA,MAAMC,aAAY,GAAI;AAClBC,OAAK,EAAE,OADW;AAElBC,MAAI,EAAE,MAFY;AAGlBC,UAAQ,EAAE,UAHQ;AAIlBC,MAAI,EAAE;AAJY,CAAtB;AAMA,MAAMC,aAAY,GAAI;AAClBC,IAAE,EAAE;AACAC,UAAM,EAAE,CADR;AAEAC,WAAO,EAAE,CAFT;AAGAC,WAAO,EAAE;AAHT,GADc;AAMlBC,IAAE,EAAE;AACAC,aAAS,EAAE,MADX;AAEAF,WAAO,EAAE,QAFT;AAGAG,aAAS,EAAE;AAHX,GANc;AAWlBC,GAAC,EAAE;AACCC,UAAM,EAAE,SADT;AAECN,WAAO,EAAE,UAFV;AAGCC,WAAO,EAAE,OAHV;AAICM,SAAK,EAAE;AAJR;AAXe,CAAtB;AAkBA,eAAe;AACXC,OAAK,EAAE;AACHC,SAAK,EAAE;AACHC,UAAI,EAAEC,KADH;AAEHC,cAAQ,EAAE;AAFP,KADJ;AAKHC,eAAW,EAAE;AACTH,UAAI,EAAEI,MADG;AAETC,aAAO,EAAE;AAFA,KALV;AASHC,YAAQ,EAAE;AACNN,UAAI,EAAEI,MADA;AAENC,aAAO,EAAE;AAFH,KATP;AAaHE,YAAQ,EAAE;AACNP,UAAI,EAAEI,MADA;AAENC,aAAO,EAAE;AAFH,KAbP;AAiBHG,UAAM,EAAE;AACJR,UAAI,EAAES,MADF;AAEJJ,aAAO,EAAE,MAAMvB;AAFX,KAjBL;AAqBH4B,UAAM,EAAE;AACJV,UAAI,EAAES;AADF,KArBL;AAwBHE,wBAAoB,EAAE;AAClBX,UAAI,EAAEY,OADY;AAElBP,aAAO,EAAE;AAFS;AAxBnB,GADI;;AA8BXQ,MAAG,GAAK;AACJ,WAAO;AACHC,WAAK,EAAE,EADJ;AAEHC,cAAQ,EAAE,EAFP;AAGHC,cAAQ,EAAE,EAHP;AAIHC,aAAO,EAAE;AAJN,KAAP;AAMH,GArCU;;AAsCXC,SAAM,GAAK;AACP;AACA;AACA;AACA;AACA,QAAI,CAAC,KAAKP,oBAAV,EAAgC;AAC5B,WAAKI,QAAL,GAAgB5B,aAAa,CAACC,EAA9B;AACA,WAAK4B,QAAL,GAAgB7B,aAAa,CAACK,EAA9B;AACA,WAAKyB,OAAL,GAAe9B,aAAa,CAACQ,CAA7B;AACJ,KATO,CAUP;;;AACA,QAAI,KAAKe,MAAT,EAAiB;AACb,WAAKK,QAAL,GAAgB,EAAE,GAAG,KAAKA,QAAV;AAAoB,WAAG,KAAKL,MAAL,CAAYtB;AAAnC,OAAhB;AACA,WAAK4B,QAAL,GAAgB,EAAE,GAAG,KAAKA,QAAV;AAAoB,WAAG,KAAKN,MAAL,CAAYlB;AAAnC,OAAhB;AACA,WAAKyB,OAAL,GAAe,EAAE,GAAG,KAAKA,OAAV;AAAmB,WAAG,KAAKP,MAAL,CAAYf;AAAlC,OAAf;AACJ,KAfO,