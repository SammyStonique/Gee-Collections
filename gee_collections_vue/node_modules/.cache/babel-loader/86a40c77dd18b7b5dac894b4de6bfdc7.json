{"ast":null,"code":"export default {\n  name: 'SearchAutocomplete',\n  props: {\n    items: {\n      type: Array,\n      required: false,\n      default: () => []\n    }\n  },\n\n  data() {\n    return {\n      search: '',\n      results: [],\n      isOpen: false\n    };\n  },\n\n  mounted() {\n    document.addEventListener('click', this.handleClickOutside);\n    const testEl = 'HeLLO wOrlD';\n    console.log(testEl.toLowerCase());\n  },\n\n  destroyed() {\n    document.removeEventListener('click', this.handleClickOutside);\n  },\n\n  methods: {\n    filterResults() {\n      this.results = this.items.filter(item => item.toLowerCase().indexOf(this.search.toLowerCase()) > -1);\n    },\n\n    onChange() {\n      this.filterResults();\n      this.isOpen = true;\n    },\n\n    setResult(result) {\n      this.search = result;\n      this.isOpen = false;\n    },\n\n    handleClickOutside(event) {\n      if (!this.$el.contains(event.target)) {\n        this.isOpen = false;\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAcA,eAAe;AACbA,MAAI,EAAE,oBADO;AAEbC,OAAK,EAAE;AACLC,SAAK,EAAE;AACLC,UAAI,EAAEC,KADD;AAELC,cAAQ,EAAE,KAFL;AAGLC,aAAO,EAAE,MAAM;AAHV;AADF,GAFM;;AASbC,MAAI,GAAG;AACL,WAAO;AACLC,YAAM,EAAE,EADH;AAELC,aAAO,EAAE,EAFJ;AAGLC,YAAM,EAAE;AAHH,KAAP;AAKD,GAfY;;AAgBbC,SAAO,GAAG;AACRC,YAAQ,CAACC,gBAAT,CAA0B,OAA1B,EAAmC,KAAKC,kBAAxC;AACA,UAAMC,MAAK,GAAI,aAAf;AACAC,WAAO,CAACC,GAAR,CAAYF,MAAM,CAACG,WAAP,EAAZ;AACD,GApBY;;AAqBbC,WAAS,GAAG;AACVP,YAAQ,CAACQ,mBAAT,CAA6B,OAA7B,EAAsC,KAAKN,kBAA3C;AACD,GAvBY;;AAwBbO,SAAO,EAAE;AACPC,iBAAa,GAAG;AACd,WAAKb,OAAL,GAAe,KAAKP,KAAL,CAAWqB,MAAX,CAAkBC,IAAG,IAAKA,IAAI,CAACN,WAAL,GAAmBO,OAAnB,CAA2B,KAAKjB,MAAL,CAAYU,WAAZ,EAA3B,IAAwD,CAAC,CAAnF,CAAf;AACD,KAHM;;AAIPQ,YAAQ,GAAG;AACT,WAAKJ,aAAL;AACA,WAAKZ,MAAL,GAAc,IAAd;AACD,KAPM;;AAQPiB,aAAS,CAACC,MAAD,EAAS;AAChB,WAAKpB,MAAL,GAAcoB,MAAd;AACA,WAAKlB,MAAL,GAAc,KAAd;AACD,KAXM;;AAYPI,sBAAkB,CAACe,KAAD,EAAQ;AACxB,UAAI,CAAC,KAAKC,GAAL,CAASC,QAAT,CAAkBF,KAAK,CAACG,MAAxB,CAAL,EAAsC;AACpC,aAAKt