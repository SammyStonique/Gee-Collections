ewVar[0]);\n    this.detailProducts = anotherVar[0];\n    Swiper.use(Autoplay); //Swiper for additional products\n\n    const swiper = new Swiper(\".swiper\", {\n      direction: \"horizontal\",\n      loop: true,\n      slidesPerView: 1,\n      speed: 700,\n      modules: [Autoplay],\n      autoplay: {\n        delay: 5000,\n        disableOnInteraction: false,\n        pauseOnMouseEnter: true\n      }\n    });\n    swiper.autoplay.start(); //Swiper for related products\n\n    const swiper1 = new Swiper(\".swiper-related\", {\n      direction: \"horizontal\",\n      loop: false,\n      slidesPerView: 3,\n      spaceBetween: 20,\n      speed: 700,\n      modules: [Autoplay],\n      autoplay: {\n        delay: 5000,\n        disableOnInteraction: false,\n        pauseOnMouseEnter: true\n      }\n    });\n    swiper1.autoplay.start();\n  }\n\n};","map":{"version":3,"mappings":"AAyRA,OAAOA,WAAP,MAAwB,8BAAxB;AACA,SAASC,MAAT,EAAiBC,QAAjB,QAAiC,QAAjC;AACA,OAAO,aAAP;AAEA,eAAe;AACbC,OAAK,EAAE,CACL,OADK,EAEL,OAFK,EAGL,aAHK,EAIL,WAJK,EAKL,mBALK,EAML,eANK,EAOL,gBAPK,EAQL,QARK,EASL,aATK,CADM;AAYbC,YAAU,EAAE;AACVJ,eADU;AAEVC;AAFU,GAZC;;AAgBbI,MAAI,GAAG;AACL,WAAO;AACLC,cAAQ,EAAE,CADL;AAELC,oBAAc,EAAE;AAFX,KAAP;AAID,GArBY;;AAsBbC,UAAQ,EAAE;AACRC,wBAAoB,GAAG;AACrB,aAAO,KAAKF,cAAL,CAAoBG,KAApB,GAA4B,CAAC,GAApC;AACD;;AAHO,GAtBG;AA2BbC,SAAO,EAAE;AACPC,qBAAiB,GAAG;AAClB,WAAKN,QAAL,IAAiB,CAAjB;AACD,KAHM;;AAIPO,qBAAiB,GAAG;AAClB,UAAIC,KAAK,CAAC,KAAKR,QAAN,CAAL,IAAwB,KAAKA,QAAL,GAAgB,CAA5C,EAA+C;AAC7C,aAAKA,QAAL,GAAgB,CAAhB;AACF,OAFA,MAEO;AACL,YAAI,KAAKA,QAAL,GAAgB,CAApB,EAAuB;AACrB,eAAKA,QAAL,IAAiB,CAAjB;AACF;AACF;AACD,KAZM;;AAaPS,oBAAgB,GAAG;AACjB;AACA,UAAID,KAAK,CAAC,KAAKR,QAAN,CAAL,IAAwB,KAAKA,QAAL,GAAgB,CAA5C,EAA+C;AAC7C,aAAKA,QAAL,GAAgB,CAAhB;AACF;;AACA,YAAMU,QAAO,GAAI;AACfC,aAAK,EAAE,KAAKC,cADG;AAEfZ,gBAAQ,EAAE,KAAKA;AAFA,OAAjB;AAIA,WAAKa,MAAL,CAAYC,MAAZ,CAAmB,WAAnB,EAAgCJ,QAAhC;AACA,WAAKK,MAAL,CAAYC,OAAZ,CAAqB,GAAE,KAAKJ,cAAL,CAAoBK,IAAI,gBAA/C;AACD,KAxBM;;AAyBPC,iBAAa,GAAG;AACd,U