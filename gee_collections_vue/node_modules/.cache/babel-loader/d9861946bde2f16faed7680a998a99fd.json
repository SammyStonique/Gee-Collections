{"ast":null,"code":"export default {\n  data() {\n    return {\n      first_name: '',\n      last_name: '',\n      email: '',\n      password: '',\n      password2: '',\n      phone_number: '',\n      errors: []\n    };\n  },\n\n  methods: {\n    createUser() {\n      this.errors = [];\n\n      if (this.first_name === '' && this.last_name === '' && this.email === '' && this.password === '' && this.password2 === '') {\n        this.errors.push('Please fill in the details!');\n      } else {\n        if (this.first_name === '') {\n          this.errors.push('First Name is missing');\n        }\n\n        if (this.last_name === '') {\n          this.errors.push('Last Name is missing');\n        }\n\n        if (this.email === '') {\n          this.errors.push('Email is missing');\n        }\n\n        if (this.phone_number === '') {\n          this.errors.push('Phone number required');\n        }\n\n        if (this.password === '') {\n          this.errors.push('Password is too short');\n        }\n\n        if (this.password2 === '') {\n          this.errors.push('Please confirm your password');\n        }\n\n        if (this.password !== this.password2) {\n          this.errors.push('Password do not match');\n        }\n      }\n\n      if (!this.errors.length) {\n        const formData = {\n          username: this.email,\n          password: this.password\n        };\n      }\n\n      this.axios.post('http://127.0.0.1:8000/api/v1/users/', formData).then(response).catch(error);\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAuFA,eAAe;AACXA,MAAI,GAAE;AACF,WAAM;AACFC,gBAAU,EAAC,EADT;AAEFC,eAAS,EAAC,EAFR;AAGFC,WAAK,EAAE,EAHL;AAIFC,cAAQ,EAAE,EAJR;AAKFC,eAAS,EAAE,EALT;AAMFC,kBAAY,EAAE,EANZ;AAOFC,YAAM,EAAC;AAPL,KAAN;AASH,GAXU;;AAYXC,SAAO,EAAC;AACJC,cAAU,GAAE;AACR,WAAKF,MAAL,GAAc,EAAd;;AACA,UAAG,KAAKN,UAAL,KAAoB,EAApB,IAAyB,KAAKC,SAAL,KAAmB,EAA5C,IAAgD,KAAKC,KAAL,KAAe,EAA/D,IAAmE,KAAKC,QAAL,KAAkB,EAArF,IAAyF,KAAKC,SAAL,KAAmB,EAA/G,EAAkH;AAC9G,aAAKE,MAAL,CAAYG,IAAZ,CAA