{"ast":null,"code":"import paginate from \"jw-paginate\";\nconst defaultLabels = {\n  first: \"First\",\n  last: \"Last\",\n  previous: \"Previous\",\n  next: \"Next\"\n};\nconst defaultStyles = {\n  ul: {\n    margin: 0,\n    padding: 0,\n    display: \"inline-block\"\n  },\n  li: {\n    listStyle: \"none\",\n    display: \"inline\",\n    textAlign: \"center\"\n  },\n  a: {\n    cursor: \"pointer\",\n    padding: \"6px 12px\",\n    display: \"block\",\n    float: \"left\"\n  }\n};\nexport default {\n  props: {\n    items: {\n      type: Array,\n      required: true\n    },\n    initialPage: {\n      type: Number,\n      default: 1\n    },\n    pageSize: {\n      type: Number,\n      default: 10\n    },\n    maxPages: {\n      type: Number,\n      default: 10\n    },\n    labels: {\n      type: Object,\n      default: () => defaultLabels\n    },\n    styles: {\n      type: Object\n    },\n    disableDefaultStyles: {\n      type: Boolean,\n      default: false\n    }\n  },\n\n  data() {\n    return {\n      pager: {},\n      ulStyles: {},\n      liStyles: {},\n      aStyles: {}\n    };\n  },\n\n  created() {\n    // if (!this.$listeners.changePage) {\n    //     throw 'Missing required event listener: \"changePage\"';\n    // }\n    // set default styles unless disabled\n    if (!this.disableDefaultStyles) {\n      this.ulStyles = defaultStyles.ul;\n      this.liStyles = defaultStyles.li;\n      this.aStyles = defaultStyles.a;\n    } // merge custom styles with default styles\n\n\n    if (this.styles) {\n      this.ulStyles = { ...this.ulStyles,\n        ...this.styles.ul\n      };\n      this.liStyles = { ...this.liStyles,\n        ...this.styles.li\n      };\n      this.aStyles = { ...this.aStyles,\n        ...this.styles.a\n      };\n    } // set to initial page\n\n\n    this.setPage(this.initialPage);\n  },\n\n  methods: {\n    setPage(page) {\n      const {\n        items,\n        pageSize,\n        maxPages\n      } = this; // get new pager object for specified page\n\n      const pager = paginate(items.length, page, pageSize, maxPages); // get new page of items from items array\n\n      const pageOfItems = items.slice(pager.startIndex, pager.endIndex + 1); // update pager\n\n      this.pager = pager; // emit change page event to parent component\n\n      this.$emit(\"changePage\", pageOfItems);\n    }\n\n  },\n  watch: {\n    items() {\n      this.setPage(this.initialPage);\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA6DA,OAAOA,QAAP,MAAqB,aAArB;AACA,MAAMC,aAAY,GAAI;AACpBC,OAAK,EAAE,OADa;AAEpBC,MAAI,EAAE,MAFc;AAGpBC,UAAQ,EAAE,UAHU;AAIpBC,MAAI,EAAE;AAJc,CAAtB;AAMA,MAAMC,aAAY,GAAI;AACpBC,IAAE,EAAE;AACFC,UAAM,EAAE,CADN;AAEFC,WAAO,EAAE,CAFP;AAGFC,WAAO,EAAE;AAHP,GADgB;AAMpBC,IAAE,EAAE;AACFC,aAAS,EAAE,MADT;AAEFF,WAAO,EAAE,QAFP;AAGFG,aAAS,EAAE;AAHT,GANgB;AAWpBC,GAAC,EAAE;AACDC,UAAM,EAAE,SADP;AAEDN,WAAO,EAAE,UAFR;AAGDC,WAAO,EAAE,OAHR;AAIDM,SAAK,EAAE;AAJN;AAXiB,CAAtB;AAkBA,eAAe;AACbC,OAAK,EAAE;AACLC,SAAK,EAAE;AACLC,UAAI,EAAEC,KADD;AAELC,cAAQ,EAAE;AAFL,KADF;AAKLC,eAAW,EAAE;AACXH,UAAI,EAAEI,MADK;AAEXC,aAAO,EAAE;AAFE,KALR;AASLC,YAAQ,EAAE;AACRN,UAAI,EAAEI,MADE;AAERC,aAAO,EAAE;AAFD,KATL;AAaLE,YAAQ,EAAE;AACRP,UAAI,EAAEI,MADE;AAERC,aAAO,EAAE;AAFD,KAbL;AAiBLG,UAAM,EAAE;AACNR,UAAI,EAAES,MADA;AAENJ,aAAO,EAAE,MAAMvB;AAFT,KAjBH;AAqBL4B,UAAM,EAAE;AACNV,UAAI,EAAES;AADA,KArBH;AAwBLE,wBAAoB,EAAE;AACpBX,UAAI,EAAEY,OADc;AAEpBP,aAAO,EAAE;AAFW;AAxBjB,GADM;;AA8BbQ,MAAI,GAAG;AACL,WAAO;AACLC,WAAK,EAAE,EADF;AAELC,cAAQ,EAAE,EAFL;AAGLC,cAAQ,EAAE,EAHL;AAILC,aAAO,EAAE;AAJJ,KAAP;AAMD,GArCY;;AAsCbC,SAAO,GAAG;AACR;AACA;AACA;AACA;AACA,QAAI,CAAC,KAAKP,oBAAV,EAAgC;AAC9B,WAAKI,QAAL,GAAgB5B,aAAa,CAACC,EAA9B;AACA,WAAK4B,QAAL,GAAgB7B,aAAa,CAACK,EAA9B;AACA,WAAKyB,OAAL,GAAe9B,aAAa,CAACQ,CAA7B;AACF,KATQ,CAUR;;;AACA,QAAI,KAAKe,MAAT,EAAiB;AACf,WAAKK,QAAL,GAAgB,EAAE,GAAG,KAAKA,QAAV;AAAoB,WAAG,KAAKL,MAAL,CAAYtB;AAAnC,OAAhB;AACA,WAAK4B,QAAL,GAAgB,EAAE,GAAG,KAAKA,QAAV;AAAoB,WAAG,KAAKN,MAAL,CAAYlB;AAAnC,OAAhB;AACA,WAAKyB,OAAL,GAAe,EAAE,GAAG,KAAKA,OAAV;AAAmB,WAAG,KAAKP,MAAL,CAAYf;AAAlC,OAAf;AACF,KAfQ,CAgBR;;;AACA,SAAKwB,OAAL,CAAa,KAAKhB,WAAlB;AACD,GAxDY;;AAyDbiB,SAAO,EAAE;AACPD,WAAO,CAACE,IAAD,EAAO;AACZ,YAAM;AAAEtB,aAAF;AAASO,gBAAT;AAAmBC;AAAnB,UAAgC,IAAtC,CADY,CAEZ;;AACA,YAAMO,KAAI,GAAIjC,QAAQ,CAACkB,KAAK,CAACuB,MAAP,EAAeD,IAAf,EAAqBf,QAArB,EAA+BC,QAA/B,CAAtB,CAHY,CAIZ;;AACA,YAAMgB,WAAU,GAAIxB,KAAK,CAACyB,KAAN,CAAYV,KAAK,CAACW,UAAlB,EAA8BX,KAAK,CAACY,QAAN,GAAiB,CAA/C,CAApB,CALY,CAMZ;;AACA,WAAKZ,KAAL,GAAaA,KAAb,CAPY,CAQZ;;AACA,WAAKa,KAAL,CAAW,YAAX,EAAyBJ,WAAzB;AACD;;AAXM,GAzDI;AAsEbK,OAAK,EAAE;AACL7B,SAAK,GAAG;AACN,WAAKoB,OAAL,CAAa,KAAKhB,WAAlB;AACD;;AAHI;AAtEM,CAAf","names":["paginate","defaultLabels","first","last","previous","next","defaultStyles","ul","margin","padding","display","li","listStyle","textAlign","a","cursor","float","props","items","type","Array","required","initialPage","Number","default","pageSize","maxPages","labels","Object","styles","disableDefaultStyles","Boolean","data","pager","ulStyles","liStyles","aStyles","created","setPage","methods","page","length","pageOfItems","slice","startIndex","endIndex","$emit","watch"],"sourceRoot":"","sources":["/home/sammyb/gee_collections/gee_collections_vue/src/components/Pagination.vue"],"sourcesContent":["<template>\n  <div class=\"row container\">\n    <div class=\"col-md-8\">\n      <ul v-if=\"pager.pages && pager.pages.length\" class=\"pagination\" :style=\"ulStyles\">\n        <li\n          class=\"page-item first\"\n          :class=\"{ disabled: pager.currentPage === 1 }\"\n          :style=\"liStyles\"\n        >\n          <a class=\"page-link\" @click=\"setPage(1)\" :style=\"aStyles\">{{ labels.first }}</a>\n        </li>\n        <li\n          class=\"page-item previous\"\n          :class=\"{ disabled: pager.currentPage === 1 }\"\n          :style=\"liStyles\"\n        >\n          <a class=\"page-link\" @click=\"setPage(pager.currentPage - 1)\" :style=\"aStyles\">{{\n            labels.previous\n          }}</a>\n        </li>\n        <li\n          v-for=\"page in pager.pages\"\n          :key=\"page\"\n          class=\"page-item page-number\"\n          :class=\"{ active: pager.currentPage === page }\"\n          :style=\"liStyles\"\n        >\n          <a class=\"page-link\" @click=\"setPage(page)\" :style=\"aStyles\">{{ page }}</a>\n        </li>\n        <li\n          class=\"page-item next\"\n          :class=\"{ disabled: pager.currentPage === pager.totalPages }\"\n          :style=\"liStyles\"\n        >\n          <a class=\"page-link\" @click=\"setPage(pager.currentPage + 1)\" :style=\"aStyles\">{{\n            labels.next\n          }}</a>\n        </li>\n        <li\n          class=\"page-item last\"\n          :class=\"{ disabled: pager.currentPage === pager.totalPages }\"\n          :style=\"liStyles\"\n        >\n          <a class=\"page-link\" @click=\"setPage(pager.totalPages)\" :style=\"aStyles\">{{\n            labels.last\n          }}</a>\n        </li>\n      </ul>\n    </div>\n    <div class=\"col-md-4\" v-if=\"pager.pages.length\">\n      <p style=\"font-size: 12px\">\n        Showing page {{ pager.currentPage }} of {{ pager.pages.length }}\n      </p>\n    </div>\n    <div class=\"col-md-4\" v-if=\"pager.pages.length\">\n      <p style=\"font-size: 12px\">Nothing to display</p>\n    </div>\n  </div>\n</template>\n\n<script>\nimport paginate from \"jw-paginate\";\nconst defaultLabels = {\n  first: \"First\",\n  last: \"Last\",\n  previous: \"Previous\",\n  next: \"Next\",\n};\nconst defaultStyles = {\n  ul: {\n    margin: 0,\n    padding: 0,\n    display: \"inline-block\",\n  },\n  li: {\n    listStyle: \"none\",\n    display: \"inline\",\n    textAlign: \"center\",\n  },\n  a: {\n    cursor: \"pointer\",\n    padding: \"6px 12px\",\n    display: \"block\",\n    float: \"left\",\n  },\n};\nexport default {\n  props: {\n    items: {\n      type: Array,\n      required: true,\n    },\n    initialPage: {\n      type: Number,\n      default: 1,\n    },\n    pageSize: {\n      type: Number,\n      default: 10,\n    },\n    maxPages: {\n      type: Number,\n      default: 10,\n    },\n    labels: {\n      type: Object,\n      default: () => defaultLabels,\n    },\n    styles: {\n      type: Object,\n    },\n    disableDefaultStyles: {\n      type: Boolean,\n      default: false,\n    },\n  },\n  data() {\n    return {\n      pager: {},\n      ulStyles: {},\n      liStyles: {},\n      aStyles: {},\n    };\n  },\n  created() {\n    // if (!this.$listeners.changePage) {\n    //     throw 'Missing required event listener: \"changePage\"';\n    // }\n    // set default styles unless disabled\n    if (!this.disableDefaultStyles) {\n      this.ulStyles = defaultStyles.ul;\n      this.liStyles = defaultStyles.li;\n      this.aStyles = defaultStyles.a;\n    }\n    // merge custom styles with default styles\n    if (this.styles) {\n      this.ulStyles = { ...this.ulStyles, ...this.styles.ul };\n      this.liStyles = { ...this.liStyles, ...this.styles.li };\n      this.aStyles = { ...this.aStyles, ...this.styles.a };\n    }\n    // set to initial page\n    this.setPage(this.initialPage);\n  },\n  methods: {\n    setPage(page) {\n      const { items, pageSize, maxPages } = this;\n      // get new pager object for specified page\n      const pager = paginate(items.length, page, pageSize, maxPages);\n      // get new page of items from items array\n      const pageOfItems = items.slice(pager.startIndex, pager.endIndex + 1);\n      // update pager\n      this.pager = pager;\n      // emit change page event to parent component\n      this.$emit(\"changePage\", pageOfItems);\n    },\n  },\n  watch: {\n    items() {\n      this.setPage(this.initialPage);\n    },\n  },\n};\n</script>\n"]},"metadata":{},"sourceType":"module"}