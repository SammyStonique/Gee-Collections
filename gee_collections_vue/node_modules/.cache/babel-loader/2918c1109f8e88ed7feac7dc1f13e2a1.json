veItem(name){\n            delete this.cart[name]\n        },\n        scrollToTop(){\n            window.scrollTo(0,0);\n        }\n    }\n}\n</script>\n\n\n<style lang=\"scss\">\n    .router-link-exact-active{\n        color: black !important;\n    }\n    .dropdown-toggle{\n        color: white !important;\n    }\n</style>"]},"metadata":{},"sourceType":"module"}