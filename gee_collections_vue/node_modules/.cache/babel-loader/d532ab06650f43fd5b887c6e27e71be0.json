{"ast":null,"code":"export default {\n  data() {\n    return {\n      myEmoji: '\\u{1F6D2}'\n    };\n  },\n\n  props: ['cart', 'items', 'remove'],\n  computed: {\n    cartItemTotal() {\n      let itemTotal = [];\n\n      for (let i = 0; i < this.cart.length; i++) {\n        let lineTotal = (this.cart[i].quantity * this.cart[i].items.price).toFixed(2);\n        itemTotal.push(lineTotal);\n      }\n\n      return itemTotal;\n    }\n\n  },\n  methods: {\n    incrementQuantity() {\n      let selectedItemQuantity = arguments[0];\n      this.cart[selectedItemQuantity].quantity += 1;\n      console.log(this.cart[selectedItemQuantity].quantity);\n    },\n\n    decrementQuantity() {\n      let selectedItemQuantity = arguments[0];\n      this.cart[selectedItemQuantity].quantity -= 1;\n      console.log(this.cart[selectedItemQuantity].quantity);\n    }\n\n  },\n\n  mounted() {}\n\n};","map":{"version":3,"mappings":"AAyFA,eAAe;AACXA,MAAI,GAAE;AACF,WAAM;AACFC,aAAO,EAAC;AADN,KAAN;AAGH,GALU;;AAMXC,OAAK,EAAC,CAAC,MAAD,EAAQ,OAAR,EAAgB,QAAhB,CANK;AAOXC,UAAQ,EAAC;AACLC,iBAAa,GAAE;AACX,UAAIC,SAAQ,GAAI,EAAhB;;AACA,WAAI,IAAIC,IAAI,CAAZ,EAAeA,CAAC,GAAC,KAAKC,IAAL,CAAUC,MAA3B,EAAmCF,CAAC,EAApC,EAAuC;AACpC,YAAIG,SAAQ,GAAI,CAAC,KAAKF,IAAL,CAAUD,CAAV,EAAaI,QAAb,GAAwB,KAAKH,IAAL,CAAUD,CAAV,EAAaK,KAAb,CAAmBC,KAA5C,EAAmDC,OAAnD,CAA2D,CAA3D,CAAhB;AACCR,iBAAS,CAACS,IAAV,CAAeL,SAAf;AACJ;;AACA,aAAOJ,SAAP;AACH;;AARI,GAPE;AAkBXU,SAAO,EAAC;AACJC,qBAAiB,GAAE;AACf,UAAIC,oBAAmB,GAAIC,SAAS,CAAC,CAAD,CAApC;AACA,WAAKX,IAAL,CAAUU,oBAAV,EAAgCP,QAAhC,IAA4C,CAA5C;AACAS,aAAO,CAACC,GAAR,CAAY,KAAKb,IAAL,CAAUU,oBAAV,EAAgCP,QAA5C;AACH,KALG;;AAMJW,qBAAiB,GAAE;AACf,UAAIJ,oBAAmB,GAAIC,SAAS,CAAC,CAAD,CAApC;AACA,WAAKX,IAAL,CAAUU,oBAAV,EAAgCP,QAAhC,IAA4C,CAA5C;AACAS,aAAO,CAACC,GAAR,CAAY,KAAKb,IAAL,CAAUU,oBAAV,EAAgCP,QAA5C;AACH;;AAVG,GAlBG;;AA+BXY,SAAO,GAAG,CAET;;AAjCU,CAAf","names":["data","myEmoji","props","computed","cartItemTotal","itemTotal","i","cart","length","lineTotal","quantity","items","price","toFixed","push","m