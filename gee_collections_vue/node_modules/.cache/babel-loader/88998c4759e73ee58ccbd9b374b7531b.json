{"ast":null,"code":"import ProductCard from '@/components/ProductCard.vue';\nimport { Swiper, Autoplay } from 'swiper';\nimport 'swiper/scss';\nexport default {\n  props: ['items', 'index', 'getProducts', 'addToCart', 'getProductDetails', 'addToWishlist', 'productDetails', 'buyNow', 'scrollToTop'],\n  components: {\n    ProductCard,\n    Swiper\n  },\n\n  data() {\n    return {\n      quantity: 1\n    };\n  },\n\n  computed: {\n    productOriginalPrice() {\n      return this.productDetails.price - -200;\n    }\n\n  },\n  methods: {\n    incrementQuantity() {\n      this.quantity += 1;\n    },\n\n    decrementQuantity() {\n      if (isNaN(this.quantity) || this.quantity < 1) {\n        this.quantity = 1;\n      } else {\n        if (this.quantity > 1) {\n          this.quantity -= 1;\n        }\n      }\n    },\n\n    detailsAddToCart() {\n      //Getting the index of the items in the cart\n      if (isNaN(this.quantity) || this.quantity < 1) {\n        this.quantity = 1;\n      }\n\n      const cartItem = {\n        items: this.productDetails,\n        quantity: this.quantity\n      };\n      this.$store.commit('addToCart', cartItem);\n      this.$toast.success(`${this.productDetails.name} added to cart`);\n    },\n\n    detailsBuyNow() {\n      if (isNaN(this.quantity) || this.quantity < 1) {\n        this.quantity = 1;\n      }\n\n      const cartItem = {\n        items: this.productDetails,\n        quantity: this.quantity\n      };\n      this.$store.commit('addToCart', cartItem);\n      this.$router.push('/checkout');\n      this.scrollToTop();\n    }\n\n  },\n\n  beforeMount() {},\n\n  mounted() {\n    this.getProducts();\n    this.quantity = 1;\n    console.log(this.productDetails);\n    Swiper.use(Autoplay); //Swiper for additional products\n\n    const swiper = new Swiper('.swiper', {\n      direction: 'horizontal',\n      loop: true,\n      slidesPerView: 1,\n      speed: 700,\n      modules: [Autoplay],\n      autoplay: {\n        delay: 5000,\n        disableOnInteraction: false,\n        pauseOnMouseEnter: true\n      }\n    });\n    swiper.autoplay.start(); //Swiper for related products\n\n    const swiper1 = new Swiper('.swiper-related', {\n      direction: 'horizontal',\n      loop: false,\n      slidesPerView: 3,\n      spaceBetween: 20,\n      speed: 700,\n      modules: [Autoplay],\n      autoplay: {\n        delay: 5000,\n        disableOnInteraction: false,\n        pauseOnMouseEnter: true\n      }\n    });\n    swiper1.autoplay.start();\n  }\n\n};","map":{"version":3,"mappings":"AA2PA,OAAOA,WAAP,MAAwB,8BAAxB;AACA,SAAQC,MAAR,EAAgBC,QAAhB,QAA+B,QAA/B;AACA,OAAO,aAAP;AAEA,eAAe;AACXC,OAAK,EAAC,CAAC,OAAD,EAAS,OAAT,EAAiB,aAAjB,EAA+B,WAA/B,EAA2C,mBAA3C,EAA+D,eAA/D,EAA+E,gBAA/E,EAAgG,QAAhG,EAAyG,aAAzG,CADK;AAEXC,YAAU,EAAC;AACPJ,eADO;AAEPC;AAFO,GAFA;;AAMXI,MAAI,GAAE;AACF,WAAM;AACFC,cAAQ,EAAE;AADR,KAAN;AAGH,GAVU;;AAWXC,UAAQ,EAAC;AACLC,wBAAoB,GAAE;AAClB,aAAO,KAAKC,cAAL,CAAoBC,KAApB,GAA4B,CAAC,GAApC;AACJ;;AAHK,GAXE;AAiBXC,SAAO,EAAC;AACJC,qBAAiB,GAAE;AACf,WAAKN,QAAL,IAAiB,CAAjB;AACH,KAHG;;AAIJO,qBAAiB,GAAE;AACf,UAAGC,KAAK,CAAC,KAAKR,QAAN,CAAL,IAAwB,KAAKA,QAAL,GAAc,CAAzC,EAA2C;AACvC,aAAKA,QAAL,GAAgB,CAAhB;AACJ,OAFA,MAGI;AACA,YAAG,KAAKA,QAAL,GAAgB,CAAnB,EAAqB;AACjB,eAAKA,QAAL,IAAiB,CAAjB;AACJ;AACJ;AACH,KAbG;;AAcJS,oBAAgB,GAAE;AACd;AACA,UAAGD,KAAK,CAAC,KAAKR,QAAN,CAAL,IAAwB,KAAKA,QAAL,GAAc,CAAzC,EAA2C;AACvC,aAAKA,QAAL,GAAgB,CAAhB;AACJ;;AACA,YAAMU,QAAQ,GAAC;AACXC,aAAI,EAAI,KAAKR,cADF;AAEXH,gBAAO,EAAI,KAAKA;AAFL,OAAf;AAIA,WAAKY,MAAL,CAAYC,MAAZ,CAAmB,WAAnB,EAA+BH,QAA/B;AACA,WAAKI,MAAL,CAAYC,OAAZ,CAAqB,GAAE,KAAKZ,cAAL,CAAoBa,IAAI,gBAA/C;AACH,KAzBG;;AA0BJC,iBAAa,GAAE;AACX,UAAGT,KAAK,CAAC,KAAKR,QAAN,CAAL,IAAwB,KAAKA,QAAL,GAAc,CAAzC,EAA2C;AACvC,aAAKA,QAAL,GAAgB,CAAhB;AACJ;;AAEA,YAAMU,QAAQ,GAAC;AACXC,aAAI,EAAI,KAAKR,cADF;AAEXH,gBAAO,EAAI,KAAKA;AAFL,OAAf;AAIA,WAAKY,MAAL,CAAYC,MAAZ,CAAmB,WAAnB,EAA+BH,QAA/B;AACA,WAAKQ,OAAL,CAAaC,IAAb,CAAkB,WAAlB;AACA,WAAKC,WAAL;AACH;;AAtCG,GAjBG;;AAyDXC,aAAW,GAAE,CAEZ,CA3DU;;AA4DXC,SAAO,GAAE;AACL,SA