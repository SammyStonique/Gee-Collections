   <span\n                          >ksh. {{ Number(productOriginalPrice).toLocaleString() }}</span\n                        >\n                      </p>\n                    </div>\n                    <div class=\"quantity\">\n                      <h4>Quantity:</h4>\n                      <div class=\"qty\">\n                        <button class=\"btn-minus\" @click=\"decrementQuantity()\">\n                          <i class=\"fa fa-minus\"></i>\n                        </button>\n                        <input type=\"text\" v-model=\"quantity\" />\n                        <button class=\"btn-plus\" @click=\"incrementQuantity()\">\n                          <i class=\"fa fa-plus\"></i>\n                        </button>\n                      </div>\n                    </div>\n                    <div class=\"p-size\">\n                      <h4>Size:</h4>\n                      <div class=\"btn-group btn-group-sm\">\n                        <button type=\"button\" class=\"btn\">S</button>\n                        <button type=\"button\" class=\"btn\">M</button>\n                        <button type=\"button\" class=\"btn\">L</button>\n                        <button type=\"button\" class=\"btn\">XL</button>\n                      </div>\n                    </div>\n                    <div class=\"p-color\">\n                      <h4>Color:</h4>\n                      <div class=\"btn-group btn-group-sm\">\n                        <button type=\"button\" class=\"btn\">White</button>\n                        <button type=\"button\" class=\"btn\">Black</button>\n                        <button type=\"button\" class=\"btn\">Blue</button>\n                      </div>\n                    </div>\n                    <div class=\"action\">\n                      <a class=\"btn details-add-to-cart-btn\" @click=\"detailsAddToCart()\"\n                        ><i class=\"fa fa-shopping-cart\"></i>Add to Cart</a\n                      >\n                      <a class=\"btn details-add-to-cart-btn\" @click=\"detailsBuyNow()\"\n                        ><i class=\"fa fa-shopping-bag\"></i>Buy Now</a\n                      >\n                    </div>\n                  </div>\n                </div>\n              </div>\n            </div>\n\n            <div class=\"row product-detail-bottom\">\n              <div class=\"col-lg-12\">\n                <ul class=\"nav nav-pills nav-justified\">\n                  <li class=\"nav-item\">\n                    <a class=\"nav-link active\" data-toggle=\"pill\" href=\"#description\"\n                      >Description</a\n                    >\n                  </li>\n                  <li class=\"nav-item\">\n                    <a class=\"nav-link\" data-toggle=\"pill\" href=\"#specification\"\n                      >Specification</a\n                    >\n                  </li>\n                  <li class=\"nav-item\">\n                    <a class=\"nav-link\" data-toggle=\"pill\" href=\"#reviews\">Reviews (1)</a>\n                  </li>\n                </ul>\n\n                <div class=\"tab-content\">\n                  <div id=\"description\" class=\"container tab-pane active\">\n                    <h4>Product description</h4>\n                    <p>{{ detailProducts.description }}</p>\n                  </div>\n                  <div id=\"specification\" class=\"container tab-pane fade\">\n                    <h4>Product specification</h4>\n                    <ul>\n                      <li>Lorem ipsum dolor sit amet</li>\n                      <li>Lorem ipsum dolor sit amet</li>\n                      <li>Lorem ipsum dolor sit amet</li>\n                      <li>Lorem ipsum dolor sit amet</li>\n                      <li>Lorem ipsum dolor sit amet</li>\n                    </ul>\n                  </div>\n                  <div id=\"reviews\" class=\"container tab-pane fade\">\n                    <div class=\"reviews-submitted\">\n                      <div class=\"reviewer\">\n                        Phasellus Gravida - <span>01 Jan 2020</span>\n                      </div>\n                      <div class=\"ratting\">\n                        <i class=\"fa fa-star\"></i>\n                        <i class=\"fa fa-star\"></i>\n                        <i class=\"fa fa-star\"></i>\n                        <i class=\"fa fa-star\"></i>\n                        <i class=\"fa fa-star\"></i>\n                      </div>\n                      <p>\n                        Sed ut perspiciatis unde omnis iste natus error sit voluptatem\n                        accusantium doloremque laudantium, totam rem aperiam.\n                      </p>\n                    </div>\n                    <div class=\"reviews-submit\">\n                      <h4>Give your Review:</h4>\n                      <div class=\"ratting\">\n                        <i class=\"far fa-star\"></i>\n                        <i class=\"far fa-star\"></i>\n                        <i class=\"far fa-star\"></i>\n                        <i class=\"far fa-star\"></i>\n                        <i class=\"far fa-star\"></i>\n                      </div>\n                      <div class=\"row form\">\n                        <div class=\"col-sm-6\">\n                          <input type=\"text\" placeholder=\"Name\" />\n                        </div>\n                        <div class=\"col-sm-6\">\n                          <input type=\"email\" placeholder=\"Email\" />\n                        </div>\n                        <div class=\"col-sm-12\">\n                          <textarea placeholder=\"Review\"></textarea>\n                        </div>\n                        <div class=\"col-sm-12\">\n                          <button>Submit</button>\n                        </div>\n                      </div>\n                    </div>\n                  </div>\n                </div>\n              </div>\n            </div>\n\n            <div class=\"product\">\n              <div class=\"section-header\">\n                <h1>Related Products</h1>\n              </div>\n              <div class=\"related-products\">\n                <div class=\"swiper-related\">\n                  <div class=\"swiper-wrapper\">\n                    <div class=\"swiper-slide\" v-for=\"(item, index) in items.slice(3, 6)\">\n                      <ProductCard\n                        :item=\"item\"\n                        :getProducts=\"getProducts\"\n                        :key=\"item.id\"\n                        :index=\"index\"\n                        :addToCart=\"addToCart\"\n                        :getProductDetails=\"getProductDetails\"\n                        :buyNow=\"buyNow\"\n                      />\n                    </div>\n                  </div>\n                </div>\n              </div>\n            </div>\n          </div>\n\n          <!-- Side Bar Start -->\n          <div class=\"col-lg-4 sidebar\">\n            <div class=\"sidebar-widget category\">\n              <h2 class=\"title\">Category</h2>\n              <nav class=\"navbar bg-light\">\n                <ul class=\"navbar-nav\">\n                  <li class=\"nav-item\">\n                    <a class=\"nav-link\" href=\"#\"\n                      ><i class=\"fa fa-female\"></i>Fashion & Beauty</a\n                    >\n                  </li>\n                  <li class=\"nav-item\">\n                    <a class=\"nav-link\" href=\"#\"\n                      ><i class=\"fa fa-child\"></i>Kids & Babies Clothes</a\n                    >\n                  </li>\n                  <li class=\"nav-item\">\n                    <a class=\"nav-link\" href=\"#\"\n                      ><i class=\"fa fa-tshirt\"></i>Men & Women Clothes</a\n                    >\n                  </li>\n                  <li class=\"nav-item\">\n                    <a class=\"nav-link\" href=\"#\"\n                      ><i class=\"fa fa-mobile-alt\"></i>Gadgets & Accessories</a\n                    >\n                  </li>\n                  <li class=\"nav-item\">\n                    <a class=\"nav-link\" href=\"#\"\n                      ><i class=\"fa fa-microchip\"></i>Electronics & Accessories</a\n                    >\n                  </li>\n                </ul>\n              </nav>\n            </div>\n            <div class=\"sidebar-widget widget-slider\">\n              <div class=\"swiper\">\n                <div class=\"swiper-wrapper\">\n                  <div class=\"swiper-slide\" v-for=\"(item, index) in items.slice(0, 3)\">\n                    <ProductCard\n                      :item=\"item\"\n                      :getProducts=\"getProducts\"\n                      :key=\"item.id\"\n                      :index=\"index\"\n                      :addToCart=\"addToCart\"\n                      :addToWishlist=\"addToWishlist\"\n                      :getProductDetails=\"getProductDetails\"\n                      :buyNow=\"buyNow\"\n                    />\n                  </div>\n                </div>\n              </div>\n            </div>\n            <div class=\"sidebar-widget brands\">\n              <h2 class=\"title\">Our Brands</h2>\n              <ul>\n                <li><a href=\"#\">Nulla </a><span>(45)</span></li>\n                <li><a href=\"#\">Curabitur </a><span>(34)</span></li>\n                <li><a href=\"#\">Nunc </a><span>(67)</span></li>\n                <li><a href=\"#\">Ullamcorper</a><span>(74)</span></li>\n                <li><a href=\"#\">Fusce </a><span>(89)</span></li>\n                <li><a href=\"#\">Sagittis</a><span>(28)</span></li>\n              </ul>\n            </div>\n\n            <div class=\"sidebar-widget tag\">\n              <h2 class=\"title\">Tags Cloud</h2>\n              <a href=\"#\">Lorem ipsum</a>\n              <a href=\"#\">Vivamus</a>\n              <a href=\"#\">Phasellus</a>\n              <a href=\"#\">pulvinar</a>\n              <a href=\"#\">Curabitur</a>\n              <a href=\"#\">Fusce</a>\n              <a href=\"#\">Sem quis</a>\n              <a href=\"#\">Mollis metus</a>\n              <a href=\"#\">Sit amet</a>\n              <a href=\"#\">Vel posuere</a>\n              <a href=\"#\">orci luctus</a>\n              <a href=\"#\">Nam lorem</a>\n            </div>\n          </div>\n          <!-- Side Bar End -->\n        </div>\n      </div>\n    </div>\n    <!-- Product Detail End -->\n  </div>\n</template>\n\n<script>\nimport ProductCard from \"@/components/ProductCard.vue\";\nimport { Swiper, Autoplay } from \"swiper\";\nimport \"swiper/scss\";\n\nexport default {\n  props: [\n    \"items\",\n    \"index\",\n    \"getProducts\",\n    \"addToCart\",\n    \"getProductDetails\",\n    \"addToWishlist\",\n    \"productDetails\",\n    \"buyNow\",\n    \"scrollToTop\"\n  ],\n  components: {\n    ProductCard,\n    Swiper,\n  },\n  data() {\n    return {\n      quantity: 1,\n      detailProducts: [],\n    };\n  },\n  computed: {\n    productOriginalPrice() {\n      return this.detailProducts.price - -200;\n    },\n  },\n  methods: {\n    incrementQuantity() {\n      this.quantity += 1;\n    },\n    decrementQuantity() {\n      if (isNaN(this.quantity) || this.quantity < 1) {\n        this.quantity = 1;\n      } else {\n        if (this.quantity > 1) {\n          this.quantity -= 1;\n        }\n      }\n    },\n    detailsAddToCart() {\n      //Getting the index of the items in the cart\n      if (isNaN(this.quantity) || this.quantity < 1) {\n        this.quantity = 1;\n      }\n      const cartItem = {\n        items: this.productDetails,\n        quantity: this.quantity,\n      };\n      this.$store.commit(\"addToCart\", cartItem);\n      this.$toast.success(`${this.productDetails.name} added to cart`);\n    },\n    detailsBuyNow() {\n      if (isNaN(this.quantity) || this.quantity < 1) {\n        this.quantity = 1;\n      }\n\n      const cartItem = {\n        items: this.productDetails,\n        quantity: this.quantity,\n      };\n      this.$store.commit(\"addToCart\", cartItem);\n      this.$router.push(\"/checkout\");\n      this.scrollToTop();\n    },\n    scrollToTop() {\n      window.scrollTo({ top: 0, behavior: \"smooth\" });\n    },\n  },\n  beforeMount() {\n    this.scrollToTop();\n  },\n  mounted() {\n    this.getProducts();\n    this.quantity = 1;\n    let newVar = JSON.parse(localStorage.getItem(\"detailProducts\"));\n    let anotherVar = [];\n    anotherVar.push(newVar[0]);\n    this.detailProducts = anotherVar[0];\n\n    Swiper.use(Autoplay);\n    //Swiper for additional products\n    const swiper = new Swiper(\".swiper\", {\n      direction: \"horizontal\",\n      loop: true,\n      slidesPerView: 1,\n      speed: 700,\n      modules: [Autoplay],\n      autoplay: {\n        delay: 5000,\n        disableOnInteraction: false,\n        pauseOnMouseEnter: true,\n      },\n    });\n    swiper.autoplay.start();\n    //Swiper for related products\n    const swiper1 = new Swiper(\".swiper-related\", {\n      direction: \"horizontal\",\n      loop: false,\n      slidesPerView: 3,\n      spaceBetween: 20,\n      speed: 700,\n      modules: [Autoplay],\n      autoplay: {\n        delay: 5000,\n        disableOnInteraction: false,\n        pauseOnMouseEnter: true,\n      },\n    });\n    swiper1.autoplay.start();\n  },\n};\n</script>\n\n<style scoped>\n.product-detail .product-slider-single-nav {\n  margin: 15px 30px 30px 30px;\n  border: 3px double #ff6f61;\n}\n.product-detail-slider {\n  height: 421px;\n}\n.details-add-to-cart-btn {\n  color: #ff6f61;\n}\n.details-add-to-cart-btn:hover {\n  color: white;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}