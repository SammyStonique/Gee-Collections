{"ast":null,"code":"export default {\n  name: 'SearchAutocomplete',\n  props: {\n    items: {\n      type: Array,\n      required: false,\n      default: () => []\n    }\n  },\n\n  data() {\n    return {\n      search: '',\n      results: [],\n      isOpen: false,\n      arrowCounter: -1\n    };\n  },\n\n  beforeMount() {\n    this.search = this.$store.state.productSearch;\n    console.log('store value', this.$store.state.productSearch);\n  },\n\n  mounted() {\n    document.addEventListener('click', this.handleClickOutside);\n  },\n\n  updated() {\n    this.$nextTick(() => {\n      this.$store.state.productSearch = this.search;\n      console.log('store value is now', this.$store.state.productSearch);\n    });\n  },\n\n  destroyed() {\n    document.removeEventListener('click', this.handleClickOutside);\n  },\n\n  methods: {\n    filterResults() {\n      let newArray = [];\n\n      for (let i = 0; i < this.items.length; i++) {\n        newArray.push(this.items[i].name);\n      }\n\n      this.results = newArray.filter(item => item.toLowerCase().indexOf(this.search.toLowerCase()) > -1); // this.results = this.items.filter(item => item.toLowerCase().indexOf(this.search.toLowerCase()) > -1);\n    },\n\n    onChange() {\n      this.filterResults();\n      this.isOpen = true;\n    },\n\n    setResult(result) {\n      this.search = result;\n      this.isOpen = false;\n      this.$router.push('/search'); // this.emitter.emit('my-event', {'eventContent': this.search})\n      // console.log(this.search)\n    },\n\n    handleClickOutside(event) {\n      if (!this.$el.contains(event.target)) {\n        this.arrowCounter = -1;\n        this.isOpen = false;\n      }\n    },\n\n    onArrowDown() {\n      if (this.arrowCounter < this.results.length) {\n        this.arrowCounter = this.arrowCounter + 1;\n      }\n    },\n\n    onArrowUp() {\n      if (this.arrowCounter > 0) {\n        this.arrowCounter = this.arrowCounter - 1;\n      }\n    },\n\n    onEnter() {\n      this.search = this.results[this.arrowCounter];\n      this.arrowCounter = -1;\n      this.isOpen = false;\n      this.emitter.emit('my-event', {\n        'eventContent': 'Success'\n      });\n      this.$router.push('/search');\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAuBA,eAAe;AACbA,MAAI,EAAE,oBADO;AAEbC,OAAK,EAAE;AACLC,SAAK,EAAE;AACLC,UAAI,EAAEC,KADD;AAELC,cAAQ,EAAE,KAFL;AAGLC,aAAO,EAAE,MAAM;AAHV;AADF,GAFM;;AASbC,MAAI,GAAG;AACL,WAAO;AACLC,YAAM,EAAE,EADH;AAELC,aAAO,EAAE,EAFJ;AAGLC,YAAM,EAAE,KAHH;AAILC,kBAAY,EAAE,CAAC;AAJV,KAAP;AAMD,GAhBY;;AAiBbC,aAAW,GAAE;AACX,SAAKJ,MAAL,GAAc,KAAKK,MAAL,CAAYC,KAAZ,CAAkBC,aAAhC;AACAC,WAAO,CAACC,GAAR,CAAY,aAAZ,EAA0B,KAAKJ,MAAL,CAAYC,KAAZ,CAAkBC,aAA5C;AACD,GApBY;;AAqBbG,SAAO,GAAG;AACRC,YAAQ,CAACC,gBAAT,CAA0B,OAA1B,EAAmC,KAAKC,kBAAxC;AACD,GAvBY;;AAwBbC,SAAO,GAAE;AACP,SAAKC,SAAL,CAAe,MAAI;AACjB,WAAKV,MAAL,CAAYC,KAAZ,CAAkBC,aAAlB,GAAkC,KAAKP,MAAvC;AACAQ,aAAO,CAACC,GAAR,CAAY,oBAAZ,EAAiC,KAAKJ,MAAL,CAAYC,KAAZ,CAAkBC,aAAnD;AACD,KAHD;AAID,GA7BY;;AA8BbS,WAAS,GAAG;AACVL,YAAQ,CAACM,mBAAT,CAA6B,OAA7B,EAAsC,KAAKJ,kBAA3C;AACD,GAhCY;;AAiCbK,SAAO,EAAE;AACPC,iBAAa,GAAG;AACd,UAAIC,QAAO,GAAI,EAAf;;AACA,WAAK,IAAIC,CAAC,GAAC,CAAX,EAAeA,CAAC,GAAC,KAAK3B,KAAL,CAAW4B,MAA5B,EAAqCD,CAAC,EAAtC,EAAyC;AACvCD,gBAAQ,CAACG,IAAT,CAAc,KAAK7B,KAAL,CAAW2B,CAAX,EAAc7B,IAA5B;AACF;;AACA,WAAKS,OAAL,GAAemB,QAAQ,CAACI,MAAT,CAAgBC,IAAG,IAAKA,IAAI,CAACC,WAAL,GAAmBC,OAAnB,CAA2B,KAAK3B,MAAL,CAAY0B,WAAZ,EAA3B,IAAwD,CAAC,CAAjF,CAAf,CALc,CAMd;AACD,KARM;;AASPE,YAAQ,GAAG;AACT,WAAKT,aAAL;AACA,WAAKjB,MAAL,GAAc,IAAd;AACD,KAZM;;AAaP2B,aAAS,CAACC,MAAD,EAAS;AAChB,WAAK9B,MAAL,GAAc8B,MAAd;AACA,WAAK5B,MAAL,GAAc,KAAd;AACA,WAAK6B,OAAL,CAAaR,IAAb,CAAkB,SAAlB,EAHgB,CAIhB;AACA;AACD,KAnBM;;AAoBPV,sBAAkB,CAACmB,KAAD,EAAQ;AACxB,UAAI,CAAC,KAAKC,GAAL,CAASC,QAAT,CAAkBF,KAAK,CAACG,MAAxB,CAAL,EAAsC;AACpC,aAAKhC,YAAL,GAAoB,CAAC,CAArB;AACA,aAAKD,MAAL,GAAc,KAAd;AACF;AACD,KAzBM;;AA0BPkC,eAAW,GAAG;AACZ,UAAI,KAAKjC,YAAL,GAAoB,KAAKF,OAAL,CAAaqB,MAArC,EAA6C;AAC3C,aAAKnB,YAAL,GAAoB,KAAKA,YAAL,GAAoB,CAAxC;AACF;AACD,KA